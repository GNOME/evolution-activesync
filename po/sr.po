#, c-format
msgid "wbxml is NULL!"
msgstr "вбхмл је НИШТА!"

#: ../eas-daemon/libeas/eas-connection.c:1404
#, c-format
msgid "wbxml_len is 0!"
msgstr "вбхмл_лен је 0!"

#: ../eas-daemon/libeas/eas-connection.c:1415
#, c-format
msgid "Failed to create WBXML to XML converter: %s"
