#: ../eas-daemon/libeas/eas-connection.c:1320
#: ../eas-daemon/libeas/eas-connection.c:1331
#: ../eas-daemon/libeas/eas-connection.c:2661
#, c-format
msgid "HTTP request failed: %d — %s"
msgstr "Није успео ХТТП захтев: %d — %s"

